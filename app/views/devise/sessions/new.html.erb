<%= render "partials/loginhead"%>
<div class="cont1">
  <h2>Log in</h2>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <div class="mb-3">
  <%=f.label :email ,class:"form-label"%>
  <%= f.email_field :email, autofocus: true, autocomplete: "email",id:"formFile",class:"form-control"  %>
</div>
    </div>

    <div class="field">
      <%= f.label :password ,class:"form-label"%><br />
      <%= f.password_field :password, autocomplete: "current-password",id:"formFile",class:"form-control" %>
    </div>

    <% if devise_mapping.rememberable? %>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end %>

    <div class="actions">
      <br>
      <center>
      <%= f.submit "Log in",class:"btn btn-warning" %>
      </center>
      <br>
    </div>
  <% end %>

  <%= render "devise/shared/links" %>
</div>
<%= render 'partials/loginabout'%>
